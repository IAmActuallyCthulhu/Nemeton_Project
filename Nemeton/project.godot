; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Nemeton"
run/main_scene="res://Game.tscn"
config/icon="res://icon.png"

[autoload]

Notice="*res://Systems/Notice.gd"
Game="*res://Game.gd"
Party="*res://Characters/Party.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
mouse_cursor/custom_image="res://Theme/Cursor/Normal.png"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "BetterRichTextLabel", "todo" )

[rendering]

environment/default_clear_color=Color( 0.0196078, 0.0196078, 0.0196078, 1 )
environment/default_environment="res://default_env.tres"
